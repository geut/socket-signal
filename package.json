{
  "name": "socket-signal",
  "version": "10.1.0",
  "description": "Signal abstraction to create WebRTC connections through sockets.",
  "type": "module",
  "exports": {
    ".": "./src/index.js"
  },
  "files": [
    "src"
  ],
  "browserslist": [
    "> 5%"
  ],
  "scripts": {
    "start": "node index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --passWithNoTests --forceExit",
    "posttest": "npm run lint",
    "lint": "standard \"**/*.js\"",
    "prepublishOnly": "npm test"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "end-of-stream": "^1.4.4",
    "fastq": "^1.11.1",
    "nanocustomassert": "^1.0.0",
    "nanoerror": "^1.0.0",
    "nanomessage-rpc": "^5.0.0",
    "nanoresource-promise": "^3.0.1",
    "p-event": "^4.1.0",
    "p-limit": "^3.0.2",
    "simple-peer": "^9.11.0"
  },
  "devDependencies": {
    "duplexify": "^4.1.1",
    "enhanced-resolve": "^5.8.2",
    "jest": "^27.0.4",
    "standard": "^16.0.1",
    "through2": "^3.0.1",
    "wrtc": "^0.4.4"
  },
  "jest": {
    "resolver": "./tests/jest-resolver.cjs",
    "testEnvironment": "jest-environment-node",
    "transform": {},
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "standard": {
    "env": [
      "jest",
      "node",
      "browser"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geut/socket-signal.git"
  },
  "keywords": [
    "socket-signal",
    "webrtc",
    "signal",
    "signal-client",
    "signal-server",
    "simple-peer"
  ],
  "author": {
    "name": "GEUT",
    "email": "contact@geutstudio.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geut/socket-signal/issues"
  },
  "homepage": "https://github.com/geut/socket-signal#readme"
}
